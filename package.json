{
  "name": "fhevm-react-template",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:sdk": "npm run dev --workspace=packages/fhevm-sdk",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs",
    "dev:react": "npm run dev --workspace=examples/react",
    "dev:auction": "npm run dev --workspace=examples/auction-dapp",
    "compile": "cd ../.. && npm run compile",
    "deploy": "cd ../.. && npm run deploy",
    "deploy:sepolia": "cd ../.. && npm run deploy:sepolia",
    "test": "cd ../.. && npm test",
    "lint": "npm run lint --workspaces --if-present",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules packages/*/dist"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "blockchain",
    "privacy",
    "sdk",
    "react",
    "nextjs"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeyonCronin/FHEArtifactAuction.git"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
